#include<16F887.h>
#fuses INTRC_IO, NOWDT, PROTECT, NOLVP, MCLR, NOLVP, NOBROWNOUT
#use delay(INTERNAL=4000000)

#include <lcd.c>


// AQUI VAN LAS MACROS


void main()
{
   lcd_init();
   while(true)
   {
      printf(lcd_putc, "f\Hello there"); //     SE HACE CLEAR EN PANTALLA
      delay_ms(1000);
   }

}

